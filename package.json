{
  "name": "gitignore-generator",
  "author": "Ahmed Sonnenholzner",
  "publisher": "Ahmed Sonnenholzner",
  "license": "MIT",
  "displayName": "vscode-gitignore-generator",
  "description": "Generates a .gitignore file",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:Gitignore.generate"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "Gitignore.generate",
        "title": "Generate .gitignore"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "Gitignore.generate",
          "when": "explorerResourceIsFolder",
          "group": "Gitignore@1"
        }
      ]
    }
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^3.1.4",
    "vscode": "^1.1.25",
    "eslint": "^4.11.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  }
}
